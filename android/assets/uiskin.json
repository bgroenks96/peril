{
  "com.badlogic.gdx.graphics.g2d.BitmapFont": { "default-font": { "file": "default.fnt" } },

  "com.badlogic.gdx.graphics.Color": {
      "green": { "a": 1, "b": 0, "g": 1, "r": 0 },
      "white": { "a": 1, "b": 1, "g": 1, "r": 1 },
      "red":   { "a": 1, "b": 0, "g": 0, "r": 1 },
      "black": { "a": 1, "b": 0, "g": 0, "r": 0 }
  },

  "com.badlogic.gdx.scenes.scene2d.ui.Skin$TintedDrawable": {
      "dialogDim": { "name": "white", "color": { "r": 0, "g": 0, "b": 0, "a": 0.45 } }
  },

  "com.badlogic.gdx.scenes.scene2d.ui.Button$ButtonStyle": {
      "default": { "down": "button-down", "up": "button-up", "over": "button-over", "disabled": "button-disabled" },
      "toggle":  { "down": "toggle-on", "up": "toggle-off", "checked": "toggle-on", "disabled": "toggle-disabled" },
      "small":   { "down": "small-button-down", "up": "small-button-up", "over": "small-button-over", "disabled": "small-button-disabled" }
  },

  "com.badlogic.gdx.scenes.scene2d.ui.TextButton$TextButtonStyle": {
      "default": { "down": "button-down", "up": "button-up", "over": "button-over", "disabled": "button-disabled", "font": "default-font", "fontColor": "white"},
      "small":   { "down": "small-button-down", "up": "small-button-up", "over": "small-button-over", "disabled": "small-button-disabled", "font": "default-font", "fontColor": "white"}
  },

  "com.badlogic.gdx.scenes.scene2d.ui.ScrollPane$ScrollPaneStyle": {
      "default": { "vScroll": null, "hScrollKnob": "scroll-knob", "background": null, "hScroll": null, "vScrollKnob": "scroll-knob" }
  },

  "com.badlogic.gdx.scenes.scene2d.ui.SelectBox$SelectBoxStyle": {
      "default": {
          "font": "default-font",
          "fontColor": "white",
          "background": "default-select",
          "scrollStyle": "default",
          "listStyle": { "font": "default-font", "selection": "default-select-selection" }
      }
  },

  "com.badlogic.gdx.scenes.scene2d.ui.SplitPane$SplitPaneStyle": {
      "default-vertical":   { "handle": "default-splitpane-vertical" },
      "default-horizontal": { "handle": "default-splitpane" }
  },

  "com.badlogic.gdx.scenes.scene2d.ui.Window$WindowStyle": {
      "default": { "titleFont": "default-font", "background": "default-window", "titleFontColor": "white"},
      "dialog":  { "titleFont": "default-font", "background": "default-window", "titleFontColor": "white", "stageBackground": "dialogDim"}
  },

  "com.badlogic.gdx.scenes.scene2d.ui.ProgressBar$ProgressBarStyle": {
      "default-horizontal": { "background": "default-slider", "knob": "default-slider-knob" },
      "default-vertical":   { "background": "default-slider", "knob": "default-round-large" }
  },

  "com.badlogic.gdx.scenes.scene2d.ui.Slider$SliderStyle": {
    "default-horizontal": { "background": "horizontal-slider-background", "knob": "slider-knob" },
    "default-vertical":   { "background": "vertical-slider-background", "knob": "slider-knob" }
  },

  "com.badlogic.gdx.scenes.scene2d.ui.Label$LabelStyle": {
      "default": { "font": "default-font", "fontColor": "white"}
  },

  "com.badlogic.gdx.scenes.scene2d.ui.TextField$TextFieldStyle": {
      "default": { "selection": "selection", "background": "textfield-background", "focusedBackground": "textfield-focused-background", "disabledBackground": "textfield-disabled-background", "font": "default-font", "fontColor": "white", "cursor": "cursor"}
  },

  "com.badlogic.gdx.scenes.scene2d.ui.CheckBox$CheckBoxStyle": {
      "default": { "checkboxOn": "checkbox-on", "checkboxOff": "checkbox-off", "checkboxOnDisabled": "checkbox-on-disabled", "checkboxOffDisabled": "checkbox-off-disabled", "font": "default-font", "fontColor": "white"}
  },

  "com.badlogic.gdx.scenes.scene2d.ui.List$ListStyle": {
      "default": { "fontColorUnselected": "white", "selection": "selection", "fontColorSelected": "white", "font": "default-font" }
  },

  "com.badlogic.gdx.scenes.scene2d.ui.Touchpad$TouchpadStyle": {
      "default": { "background": "default-pane", "knob": "default-round-large" }
  },

  "com.badlogic.gdx.scenes.scene2d.ui.Tree$TreeStyle": {
      "default": { "minus": "tree-minus", "plus": "tree-plus", "selection": "default-select-selection" }
  }
}
